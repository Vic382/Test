# Le nom de la vue du cube :
sVueDestination = 'RàZ_vue_' | Today (0) | '_' | GetProcessName () ; 
# Le nom des sous-ensembles
sSubsetDestination = 'RàZ_Subset_' | Today (0) | '_' | GetProcessName () ; 


# Destruction pour recréation de la vue 

