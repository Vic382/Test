SKIPCHECK;

# Calcul de la valeur d'achat
['Montant'] = N : ['Quantite'] * DB( 'Param_Tarif_Sac', !Exercice, !Version, !Scenario, !Mois, !Type_Sac, 'Tarif_Achat'  ) ;

FEEDERS;

# Feeders calcul valeur achat
['Quantite'] => ['Montant'] ;

# Feeders du cube Stock_Sac
['Montant'] => DB( 'Stock_Sac', !Exercice, !Version, !Scenario, !Mois, !Type_Sac, !Magasin, 'Achat_Sac'  ) ;